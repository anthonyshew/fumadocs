{
  "name": "fumadocs-mdx",
  "version": "8.2.0",
  "description": "The built-in source for Fumadocs",
  "keywords": [
    "NextJs",
    "Docs"
  ],
  "homepage": "https://fumadocs.vercel.app",
  "repository": "github:fuma-nama/fumadocs",
  "license": "MIT",
  "author": "Fuma Nama",
  "exports": {
    "./loader": "./loader.js",
    "./loader-mdx": "./loader-mdx.js",
    "./config": {
      "import": "./dist/config.mjs",
      "types": "./dist/config.d.mts"
    },
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.mts"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.mts",
  "typesVersions": {
    "*": {
      "config": [
        "./dist/config.d.mts"
      ]
    }
  },
  "files": [
    "dist/*",
    "loader-mdx.js",
    "loader.js"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "@mdx-js/mdx": "^3.0.0",
    "cross-spawn": "^7.0.3",
    "estree-util-value-to-estree": "^3.0.1",
    "fast-glob": "^3.3.1",
    "fumadocs-core": "workspace:*",
    "gray-matter": "^4.0.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.4",
    "@types/mdast": "^4.0.3",
    "@types/mdx": "^2.0.8",
    "@types/react": "18.2.0",
    "eslint-config-custom": "workspace:*",
    "next": "14.1.2",
    "tsconfig": "workspace:*",
    "unified": "^11.0.4",
    "webpack": "^5.89.0"
  },
  "peerDependencies": {
    "next": ">= 13.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
